    <h1 id="title">Eat Your Feels!</h1>
<div class="container-fluid" id="big-box">
  <div class="row align-row">
    <div class="container-fluid md-4 sm-12" id="box" id="up">
      <h4>Your Feels!</h4>
      <ul>
        {{#each cats}}
          {{#unless sleepy}}
            {{> cats/cat-block sleep=true}}
          {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="container-fluid md-4 sm-12" id="box">
      <h4 style="text-align:center;">Add a Feel</h4>
      <br>
      <form class="create-form">
        <div class="form-group">
          <label for="ca">What are you feeling?:</label>
          <input type="text" id="ca" name="name">
        </div>
        <div class="form-group">
          <label for="sleepy">How to Deal?</label><br>
          <input type="radio" name="sleepy" value="1" checked> Personal Growth!<br>
          <input type="radio" name="sleepy" value="0"> Swallow Emotion
        </div>
        <button type="submit">Add Feeling</button>
      </form>
    </div>
    <div class="container-fluid md-4 sm-12" id="box" id="down">
        <h4>Feels pushed way, way down!</h4>
        <ul>
          {{#each cats}}
            {{#if sleepy}}
              {{> cats/cat-block sleep=false}}
            {{/if}}
          {{/each}}
        </ul>
    </div>
  </div>
</div>